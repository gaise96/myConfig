// Placer vos combinaisons de touches dans ce fichier pour remplacer les valeurs par d√©faut
[
    {
        "key": "alt+d",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorTextFocus"
    },
    {
        "key": "alt+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorTextFocus"
    },
    {
        "key": "alt+i",
        "command": "namespaceResolver.import",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+l",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "alt+m",
        "command": "editor.emmet.action.wrapWithAbbreviation",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m ctrl+m",
        "command": "editor.emmet.action.matchTag",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+l",
        "command": "editor.fold",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+m",
        "command": "editor.unfold",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m ctrl+r",
        "command": "editor.emmet.action.updateTag",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m ctrl+backspace",
        "command": "editor.emmet.action.removeTag",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m ctrl+i",
        "command": "editor.emmet.action.balanceIn",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m ctrl+o",
        "command": "editor.emmet.action.balanceOut",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+d",
        "command": "editor.action.smartSelect.expand",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+d",
        "command": "editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+j",
        "command": "editor.action.joinLines",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m ctrl+r",
        "command": "workbench.action.splitEditorRight",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m ctrl+l",
        "command": "workbench.action.splitEditorLeft",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m ctrl+right",
        "command": "workbench.action.navigateRight",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+m ctrl+left",
        "command": "workbench.action.navigateLeft",
        "when": "editorTextFocus"
    },
    /**    *   workbench    */ {
        "key": "ctrl+t",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "ctrl+t",
        "command": "-workbench.action.terminal.focus",
        "when": "terminalFocus"
    },
    {
        "key": "alt+1",
        "command": "workbench.action.toggleActivityBarVisibility"
    },
    {
        "key": "alt+2",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+k ctrl+e",
        "command": "workbench.view.explorer"
    },
    {
        "key": "ctrl+k ctrl+a",
        "command": "workbench.view.extension.thunder-client"
    },
    {
        "key": "ctrl+k ctrl+v",
        "command": "workbench.view.scm"
    },
    {
        "key": "ctrl+k ctrl+d",
        "command": "workbench.view.debug"
    },
    {
        "key": "ctrl+k ctrl+x",
        "command": "workbench.view.extensions"
    },
    {
        "key": "ctrl+k ctrl+m",
        "command": "workbench.view.extension.github-cweijan-mysql"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "ctrl+o",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.action.showAllEditorsByMostRecentlyUsed"
    },
    {
        "key": "ctrl+r",
        "command": "workbench.action.gotoSymbol"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.terminal.new",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus"
    },
    /**    *  File Explorer    */ {
        "key": "ctrl+n",
        "command": "explorer.newFile",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
    },
    {
        "key": "ctrl+shift+n",
        "command": "explorer.newFolder",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
    },
    {
        "key": "ctrl+shift+o",
        "command": "revealFileInOS"
    },
    // a revoir
    {
        "key": "alt+b alt+d",
        "command": "duplicate.execute",
        "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
    },
    {
        "key": "ctrl+k ctrl+r",
        "command": "workbench.files.action.refreshFilesExplorer",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
    },
    /**    *  File Explorer    */
    {
        "key": "ctrl+n",
        "command": "explorer.newFile",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
    },
    {
        "key": "ctrl+shift+n",
        "command": "explorer.newFolder",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
    },
    {
        "key": "ctrl+shift+o",
        "command": "revealFileInOS"
    },
    {
        "key": "alt+b alt+d",
        "command": "duplicate.execute",
        "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
    },
    {
        "key": "ctrl+k ctrl+r",
        "command": "workbench.files.action.refreshFilesExplorer",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
    },
    /**    *  Multi-cursor    */
    {
        "key": "ctrl+backspace",
        "command": "editor.action.moveSelectionToPreviousFindMatch",
        "when": "editorFocus && editorHasMultipleSelections"
    },
    {
        "key": "ctrl+k ctrl+n",
        "command": "editor.action.moveSelectionToNextFindMatch",
        "when": "editorFocus && editorHasMultipleSelections"
    },
    {
        "key": "ctrl+right",
        "command": "editor.action.insertCursorAtEndOfEachLineSelected",
        "when": "editorFocus && editorHasSelection"
    },
    {
        "key": "ctrl+alt+n",
        "command": "toggle",
        "when": "editorTextFocus",
        "args": {
            "id": "lineNumber && folding",
            "value": [
                {
                    "editor.lineNumbers": "on",
                    "editor.showFoldingControls": "always",
                },
                {
                    "editor.lineNumbers": "off",
                    "editor.showFoldingControls": "never",
                }
            ]
        }
    }
]
