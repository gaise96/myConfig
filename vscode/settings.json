{
    // breadcrumbs
    "breadcrumbs.symbolPath": "off",
    // Editor
    "editor.fontFamily": "'MonoLisa', 'ZenMonolith', 'Jetbrains Mono', 'Dank Mono', 'Geist Mono', 'Operator Mono', 'monospace'",
    "editor.fontSize": 13,
    "editor.lineHeight": 4,
    "editor.fontLigatures": true,
    "editor.fontWeight": "500",
    "editor.minimap.enabled": false,
    "editor.suggestFontSize": 12,
    "editor.showFoldingControls": "always",
    "editor.guides.indentation": false,
    "editor.links": true,
    "editor.mouseWheelZoom": true,
    "editor.quickSuggestionsDelay": 10,
    "editor.suggestLineHeight": 40,
    "editor.renderLineHighlight": "none",
    "editor.occurrencesHighlight": "off",
    "editor.renderWhitespace": "trailing",
    "editor.letterSpacing": 0.4,
    "editor.cursorStyle": "block",
    "editor.cursorBlinking": "blink",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.wordWrap": "on",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.suggest.insertMode": "replace",
    "editor.snippetSuggestions": "inline",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.folding": true,
    "editor.formatOnSave": true,
    "editor.linkedEditing": true,
    "editor.tabCompletion": "on",
    "editor.lineNumbers": "on",
    // "editor.glyphMargin": true,
    "editor.inlineSuggest.enabled": true,
    "editor.lightbulb.enabled": "off",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.overviewRulerBorder": false,
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.scrollbar.horizontalScrollbarSize": 0,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "editor.tabSize": 4,
    "editor.matchBrackets": "never",
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "editor.cursorWidth": 4,
    "editor.renderControlCharacters": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.colorDecorators": false,
    "editor.codeLens": false,
    "editor.selectionHighlight": false,
    "editor.inlineSuggest.suppressSuggestions": true,
    "editor.detectIndentation": false,
    "editor.wordWrapColumn": 175,
    "editor.hover.delay": 750,
    "editor.hover.enabled": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.defaultFormatter": "laravel.vscode-laravel",
    // window
    "window.autoDetectColorScheme": true,
    "window.titleBarStyle": "custom",
    "window.newWindowDimensions": "inherit",
    "window.commandCenter": false,
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}${separator}${appName}",
    "window.customTitleBarVisibility": "auto",
    // Workbench
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 15,
    "workbench.statusBar.visible": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.editor.enablePreview": false,
    "workbench.editor.showTabs": "single",
    "workbench.layoutControl.enabled": false,
    "workbench.tips.enabled": false,
    "workbench.editor.decorations.colors": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.tree.enableStickyScroll": false,
    // "workbench.preferredLightColorTheme": "Atom One Light",
    "workbench.preferredDarkColorTheme": "Omni",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.iconTheme": "symbols",
    "workbench.editor.pinnedTabSizing": "compact",
    "workbench.editor.highlightModifiedTabs": true,
    // Explorer
    "explorer.autoReveal": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "explorer.compactFolders": false,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
        "composer.json": "composer.lock",
        "psalm.xml": "psalm-baseline.xml",
        ".php-cs-fixer.php": ".php-cs-fixer.cache",
        "phpunit.xml": ".phpunit.result.cache",
        "*.php": "${basename}Test.php"
    },
    "files.associations": {
        "*.embeddedhtml": "html",
        ".php_cs.dist": "php",
        ".php_cs": "php",
        ".pre": "php",
        "artisan": "php"
    },

    "breadcrumbs.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.preferredLightColorTheme": "Default Dark Modern",
    // Emmet Configuration
    "emmet.triggerExpansionOnTab": true,
    "emmet.includeLanguages": {
        "blade.php": "html",
        "blade": "html",
        "vue-html": "html",
        "vue": "html",
        "javascript": "html",
        "twig": "html",
        "postcss": "scss",
        "vue-postcss": "scss"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    // Unicode Highlighting
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    // JavaScript & Import Module Specifier Preferences
    "javascript.preferences.importModuleSpecifierEnding": "js",
    "typescript.preferences.importModuleSpecifierEnding": "js",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.suggest.autoImports": true,
    "typescript.suggest.autoImports": true,
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.suggest.completeFunctionCalls": true,
    "liveServer.settings.donotVerifyTags": true,
    // Git and SCM
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "git.decorations.enabled": false,
    "scm.diffDecorations": "none",
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    // PHP
    "php.executablePath": ".config/herd-lite/bin/php",
    "php-cs-fixer.executablePath": "php-cs-fixer",
    "php-cs-fixer.onsave": true,
    "php-cs-fixer.rules": "@PSR12",
    "php-cs-fixer.config": ".php-cs-fixer.php;.php-cs-fixer.dist.php;.php_cs;.php_cs.dist",
    "php-cs-fixer.allowRisky": false,
    "php-cs-fixer.lastDownload": 1,
    "php-cs-fixer.exclude": [],
    "php-cs-fixer.autoFixByBracket": true,
    "php-cs-fixer.autoFixBySemicolon": false,
    "php-cs-fixer.formatHtml": false,
    "php-cs-fixer.documentFormattingProvider": true,
    "namespaceResolver.sortAlphabetically": true,
    // Editor Clipboard Behavior
    "editor.emptySelectionClipboard": false,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.accessibilitySupport": "off",
    "editor.padding.top": 16,
    // Diff Editor
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "diffEditor.wordWrap": "off",
    // Mouse Settings
    "editor.mouseWheelScrollSensitivity": 1.8,
    // Prettier
    "prettier.requireConfig": true,
    "prettier.useEditorConfig": false,
    "prettier.singleQuote": true,
    // Tailwind CSS
    "tailwindCSS.emmetCompletions": true,
    "tailwindCSS.colorDecorators": false,
    "tailwindCSS.experimental.classRegex": [
        "[\"'`](.*?)[\"'`]"
    ],
    "intelephense.telemetry.enabled": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    // Terminal
    "terminal.integrated.fontFamily": "Menlo for Powerline",
    "terminal.integrated.tabs.enabled": false,
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontWeight": "500",
    "terminal.integrated.lineHeight": 2,
    "terminal.integrated.cursorBlinking": false,
    "terminal.integrated.cursorStyle": "block",
    // Search
    "search.smartCase": true,
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/.tmp/": true,
        "**/dist": true,
        "**/package-json.json": true,
        "**/yarn.lock": true,
        "**/public/[abcdefghjklmnopqrstuvwxyz]*": true,
        "**/public/i[abcdefghijklmopqrstuvwxyz]*": true,
        "**/vendor/[abcdefghijkmnopqrstuvwxyz]*": true,
        "**/vendor/l[bcdefghijklmnopqrstuvwxyz]*": true,
        "storage/framework/views": true,
        "_ide_helper.php": true,
        "_ide_helper_models.php": true,
        ".php_cs.cache": true,
        ".phpstorm.meta.php": true,
        ".phpunit.result.cache": true
    },
    // Blade
    "blade.format.enable": true,
    "bladeFormatter.format.wrapAttributes": "preserve-aligned",
    "bladeFormatter.format.wrapLineLength": 9999,
    // autres
    "symbols.hidesExplorerArrows": false,
    "redhat.telemetry.enabled": true,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "extensions.ignoreRecommendations": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "html.autoClosingTags": true,
    // Files
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.defaultLanguage": "markdown",
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.output/**": true,
        "**/.yarn/**": true,
        "**/node_modules/**": true,
        "backend/vendor/**": true
    },
    "files.exclude": {
        "**/.idea": true,
        ".cache-loader/": true,
        ".gitattributes": true,
        ".gitkeep": true,
        ".travis.yml": true,
        ".vercel": true,
        ".vscode/": true,
        "coverage": true,
        "acf-json": true,
        "backend/vendor": true,
        "CODE_OF_CONDUCT.md": true,
        "config": false,
        "LICENSE.md": true,
        "package-lock.json": true,
        "phpcs.xml": true,
        ".editorconfig": true,
        "prettier.config.js": true,
        "**/README.md": true,
        "yarn.lock": true,
        "node_modules/": true,
        ".gitignore": true,
        ".output/node_modules/": false,
        ".output": true,
        "package.json": false,
        "tsconfig.json": true
    },
    // MultiCommand
    "multiCommand.commands": [
        {
            "command": "multiCommand.sidebarActivityBar",
            "sequence": [
                "workbench.action.toggleSidebarVisibility",
                "workbench.action.toggleActivityBarVisibility",
            ]
        }
    ],
    "update.mode": "manual",
    // Custome UI
    // "custom-ui-style.font.sansSerif": "Jetbrains Mono",
    // "custom-ui-style.stylesheet": {
    //     ".command-center-center": "width: auto !important;",
    //     ".quick-input-widget": "transform: translateY(-50%) !important; top: 50% !important; box-shadow: 0px 8px 32px rgba(0, 0, 0, .35) !important; padding: 10px 10px 18px 10px !important; background-image: backdrop-filter: blur(3px) !important; border-radius: 10px !important; width: 50% !important;",
    //     ".quick-input-widget::backdrop": "display: block; position: absolute; width: 100%; height: 100%; background: rgba(0, 0, 0, .15); top: 0; left: 0; backdrop-filter: blur(8px)",
    //     ".composite.title h2::before": " display: inline-block; margin-right: 6px; font-size: 1.1rem; font-weight: bold;",
    //     ".monaco-list-rows": "background: transparent !important;",
    //     ".ibwrapper": "height: 35px; font-size: 14px;",
    //     ".monaco-inputbox .idle": "monaco-inputbox idle",
    //     ".shadow": "display: none !important;",
    //     ".title > .presentation": "display: none;",
    // },
    "window.menuBarVisibility": "toggle",
    "php.problems.exclude": {
        "/": [
            "php_replace_with_arrow_fn"
        ]
    },
    "vscode_custom_css.imports": [
        "file:///home/gaise/custom/style.css"
    ],
    "workbench.activityBar.location": "hidden",
    "[blade]": {
        "editor.defaultFormatter": "shufo.vscode-blade-formatter"
    }
}
